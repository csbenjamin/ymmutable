{
  "name": "ymmutable",
  "version": "1.0.0",
  "description": "Transform yjs in immutable objects",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare" : "npm run build",
    "prepublishOnly" : "npm test && npm run lint",
    "preversion" : "npm run lint",
    "version" : "npm run format && git add -A src",
    "postversion" : "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/csbenjamin/ymmutable.git"
  },
  "keywords": [
    "yjs",
    "immutable",
    "CRDT",
    "offline",
    "offline-first",
    "shared-editing",
    "concurrency",
    "collaboration"
  ],
  "author": "Cristiano Benjamin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/csbenjamin/ymmutable/issues"
  },
  "homepage": "https://github.com/csbenjamin/ymmutable#readme",
  "devDependencies": {
    "@types/jest": "^29.0.3",
    "jest": "^29.0.3",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.8.3"
  },
  "files": [
    "lib/**/*"
  ]
}
